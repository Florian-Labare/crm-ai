APP_NAME=courtier-whisper
APP_ENV=local
APP_KEY=base64:GENERATE_NEW_KEY_WITH_php_artisan_key_generate
APP_DEBUG=true
APP_URL=http://localhost:8000

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4
BCRYPT_ROUNDS=12

# ---- OCTANE (Performance Boost) ----
OCTANE_SERVER=swoole
OCTANE_HTTPS=false
OCTANE_MAX_REQUESTS=500
OCTANE_WORKERS=auto
OCTANE_TASK_WORKERS=auto

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# ---- BASE DE DONNÉES ----
DB_CONNECTION=mysql
DB_HOST=db
DB_PORT=3306
DB_DATABASE=courtier-whisper
DB_USERNAME=root
DB_PASSWORD=your-db-password

# ---- SESSIONS ----
SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# ---- FILES, QUEUES, CACHE ----
BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
CACHE_STORE=redis
# CACHE_PREFIX=

# ---- REDIS (Cache & Queues) ----
# Utiliser 'redis' comme host dans Docker, ou '127.0.0.1' en local
REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=
REDIS_PORT=6379
REDIS_DB=0
REDIS_CACHE_DB=1

# ---- MEMCACHED (Alternative - Non utilisé actuellement) ----
MEMCACHED_HOST=127.0.0.1

# ---- MAIL ----
MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="courtier-whisper"

# ---- AWS ----
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# ---- IA / API ----
# ⚠️ IMPORTANT: Ne jamais commiter les vraies clés API
# Obtenez vos clés sur https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-proj-your-openai-api-key-here
OPENAI_PROJECT_ID=proj_your-project-id-here
TRANSCRIPTION_MODE=openai

# ---- WHISPER LOCAL ----
# Modèle Whisper local : tiny, base, small, medium, large, large-v2, large-v3
# Recommandation : base (bon compromis vitesse/qualité) ou small (meilleure qualité)
WHISPER_MODEL=base

# ---- HUGGINGFACE (Pyannote diarization) ----
# Obtenir un token sur https://huggingface.co/settings/tokens
# Nécessaire pour la diarisation (séparation courtier/client)
HUGGINGFACE_TOKEN=hf_your-huggingface-token-here

# ---- FRONTEND / VITE ----
VITE_APP_NAME="courtier-whisper"
FRONTEND_URL=http://localhost:5173

# ---- SANCTUM ----
SANCTUM_STATEFUL_DOMAINS=localhost:5173,localhost:8000,127.0.0.1:5173
